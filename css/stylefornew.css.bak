	color:#fff;
	font-size:2em;
	padding-bottom:30px;
}
.workcontain{
	padding: 200px 0px 200px;
	height:850px;
	position: relative;
}
.webcontain div{
	position:absolute;
}
.webcontain div:nth-child(2){
	top:35%;
	left:25%;
}
.webcontain div:nth-child(3){
	top:10%;
	left:65%;
}
#scrollUp{
	background-image:url("../img/top.png");
	right:20px;
	width:38px;
	height:38px;
}
section#insert0{
background-attachment:scroll;
color:#fff;
}
#insert0 .insertline{
	border:1px solid #fff;
	margin:40px auto;
	padding:5px 15px 20px 15px;
	width:80%;
}
