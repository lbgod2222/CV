	color:#fff;
	text-align:center;
	margin:0 0 1em;
	padding-top:1em;
	font-size:5em;
	letter-spacing:1px;
}
.skill{
	color:#fff;
	padding:100px 0 200px;
}

.innerskill h4{
	font-weight:bold;
	font-size:1.3em;
	margin:0;
	padding-bottom:10px;
}
#sec3{
	background-color:#C15D25;
}
